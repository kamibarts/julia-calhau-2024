#include <iostream>

int main() {
    int numero;


    std::cout << "Digite um numero para ver sua tabuada: ";
    std::cin >> numero;

    std::cout << "Tabuada do " << numero << ":\n";
    for (int i = 1; i <= 10; ++i) {
        std::cout << numero << " x " << i << " = " << numero * i << std::endl;
    }

    return 0;
}
